enclave {
	
	include "time.h"
	
	untrusted {
		void ocall_print_string([in, string] const char *str);

		void ocall_print_string_w([in, string] const char *str);

		void ocall_print_string_e([in, string] const char *str);
		
		size_t ocall_connection_send([user_check] void* ptr, [in, size = size] const char* msg, size_t size);
		
		size_t ocall_connection_receive([user_check] void* ptr, [out] char** msg);
		
		size_t ocall_connection_send_raw([user_check] void* ptr, [in, size = size] const char* msg, size_t size);
		
		size_t ocall_connection_receive_raw([user_check] void* ptr, [out, size = buf_size] char* buf, size_t buf_size);
		
		void ocall_connection_clean_recv_buffer([user_check] char* ptr);
		
		void ocall_get_system_time([out] time_t* timer);
		
		void ocall_get_system_utc_time([in] const time_t* timer, [out] struct tm* out_time);
	};

};